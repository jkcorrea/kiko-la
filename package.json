{
  "name": "kiko-la-storefront",
  "version": "1.0.0",
  "description": "Kiko LA Storefront",
  "license": "AGPL-3.0-or-later",
  "author": {
    "email": "jake@kiko-la.com",
    "name": "Jake Correa",
    "url": "https://github.com/jkcorrea"
  },
  "main": "index.tsx",
  "scripts": {
    "postinstall": "yarn codegen",
    "--dev--": "",
    "dev": "concurrently --prefix-colors 'green,yellow' 'yarn:dev:next' 'yarn:dev:codegen'",
    "dev:next": "NODE_OPTIONS='--inspect' next dev",
    "dev:codegen": "yarn codegen --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "codegen": "graphql-codegen --config .graphqlrc.js",
    "--prod--": "",
    "prenext": "yarn codegen",
    "next": "next",
    "prebuild": "yarn codegen",
    "build": "next build",
    "prestart": "yarn codegen",
    "start": "next start -p $PORT",
    "--make-it-pretty--": "",
    "format": "concurrently --prefix-colors 'yellow' 'yarn:format:*'",
    "format:eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --fix .",
    "format:package-json": "format-package -w",
    "format:prettier": "prettier --write '**/*.{js,jsx,ts,tsx,html,css,scss,gql,json,md}' '.editorconfig'",
    "__DISABLED__format:doctoc": "doctoc --maxlevel 3 --notitle  README.md",
    "lint": "concurrently --prefix-colors 'yellow' 'yarn:lint:*'",
    "lint:eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty .",
    "lint:tsc": "tsc -b",
    "__DISABLED__lint:markdown": "markdownlint --ignore '**/node_modules/**' '**/*.md'"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/react-hooks": "^4.0.0",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@types/detect-node": "^2.0.0",
    "apollo-boost": "^0.4.9",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-context": "^1.0.20",
    "apollo-link-http": "^1.5.17",
    "autoprefixer": "^10.1.0",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.9.7",
    "detect-node": "^2.0.4",
    "direct-styled": "^1.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^2.2.1",
    "next": "^10.0.4",
    "postcss": "^8.2.2",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^17.0.1",
    "react95": "^3.4.1",
    "styled-components": "^5.2.0",
    "tailwindcss": "^2.0.2",
    "unstated-next": "^1.1.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.19.0",
    "@graphql-codegen/typescript-document-nodes": "1.17.9",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.1",
    "@types/js-cookie": "^2.2.2",
    "@types/lodash": "^4.14.132",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.16",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.14",
    "@types/recompose": "^0.30.7",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-styled-components": "^1.11.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^25.0.0",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "format-package": "^6.1.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
